<?xml version="1.0" encoding="UTF-8"?>
<beans  
     xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"> 
    <bean id="add1" class="com.org.app.Address">
	    <constructor-arg value="101" type="String"/>
	    <constructor-arg value="Ars" type="String"/>
	    <constructor-arg value="1001" type="String"/>
	    <constructor-arg value="1001" type="String"/>
    </bean>
    <bean id="cus1" class="com.org.app.CustomerType">
	    <constructor-arg value="Ars" type="String"/>
    </bean>
    <bean id="pay1" class="com.org.app.PaymentDetails">
	    <constructor-arg value="101" type="double"/>
	    <constructor-arg value="Ars" type="String"/>
	    <constructor-arg value="12-11-2020" type="String"/>
    </bean>
   <bean id="pay2" class="com.org.app.PaymentDetails">
	    <constructor-arg value="101" type="double"/>
	    <constructor-arg value="Ars" type="String"/>
	    <constructor-arg value="12-11-2020" type="String"/>
    </bean>
    
    
    <bean id="off1" class="com.org.app.Bookable">
	    <constructor-arg value="101" type="int"/>
	    <constructor-arg value="Ars" type="String"/>
    </bean> 
    <bean id="off2" class="com.org.app.Bookable">
	    <constructor-arg value="101" type="int"/>
	    <constructor-arg value="Ars" type="String"/>
    </bean>  
    <bean id="ame1" class="com.org.app.Amenity">
	    <constructor-arg value="101" type="String"/>
	    <constructor-arg value="Ars" type="String"/>
    </bean> 
    <bean id="ame2" class="com.org.app.Amenity">
	    <constructor-arg value="101" type="String"/>
	    <constructor-arg value="Ars" type="String"/>
    </bean> 
    <bean id="fil1" class="com.org.app.File">
	    <constructor-arg value="101" type="String"/>
	    <constructor-arg value="Ars" type="String"/>
    </bean> 
    <bean id="fil2" class="com.org.app.File">
	    <constructor-arg value="101" type="String"/>
	    <constructor-arg value="Ars" type="String"/>
    </bean> 
    <bean id="role1" class="com.org.app.Role">
	    <constructor-arg value="101" type="String"/>
	    <constructor-arg value="Ars" type="String"/>
    </bean>  
    <bean id="role2" class="com.org.app.Role">
	    <constructor-arg value="101" type="String"/>
	    <constructor-arg value="Ars" type="String"/>
    </bean>  
    <bean id="role3" class="com.org.app.Role">
	    <constructor-arg value="101" type="String"/>
	    <constructor-arg value="Ars" type="String"/>
    </bean>  
    <bean id="UserBean" class="com.org.app.User">
		<property name="email" value="email"></property>
		<property name="password" value="p"></property>
		<property name="phonenumber" value="1000"></property>
		<property name="address" ref="add1"></property>
		<property name="verified" value="true"></property>
		<property name="roles">
		<list>
			<ref bean="role1"/>
	    	<ref bean="role2"/>
	    	<ref bean="role3"/>
	    </list>
	    </property>
     </bean>     
  
    <bean id="vendor" class = "com.org.app.Vendor" parent="UserBean">
    <property name="name" value="a"></property>
    <property name="description" value="a"></property>
    <property name="openingTime" value="15:34"></property>
    <property name="closingTime" value="15:34"></property>
    <property name="offers">
		<list>
			<ref bean="off1"/>
	    	<ref bean="off2"/>
	    </list>
	 </property>
	 <property name="images">
		<list>
			<ref bean="fil1"/>
	    	<ref bean="fil2"/>
	    </list>
	 </property>
    <property name="amenities">
		<list>
			<ref bean="ame1"/>
	    	<ref bean="ame2"/>
	    </list>
	 </property>
    </bean>   
   <bean id="customer" class = "com.org.app.Customer" parent="UserBean">
    <property name="forename" value="a"></property>
    <property name="surname" value="a"></property>
    <property name="customerType" ref="cus1"></property>
    <property name="favorites">
		<list>
			<ref bean="off1"/>
	    	<ref bean="off2"/>
	    </list>
	 </property>
	 <property name="paymentDetails">
		<list>
			<ref bean="pay1"/>
	    	<ref bean="pay2"/>
	    </list>
	 </property>
    
    </bean>   
</beans>